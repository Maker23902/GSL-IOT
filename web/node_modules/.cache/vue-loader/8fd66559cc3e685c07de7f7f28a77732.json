\"picType === 'img'\">\n            <img :src=\"avatar\" class=\"avatar\" v-if=\"userInfo.headerImg\" />\n            <img :src=\"require('@/assets/noBody.png')\" class=\"avatar\" v-else/>\n        </template>\n        <template v-if=\"picType === 'file'\">\n            <img :src=\"file\" class=\"file\"/>\n        </template>\n    </span>\n</template>\n\n<script>\nimport { mapGetters } from 'vuex'\nconst path = process.env.VUE_APP_BASE_API\nexport default {\n    name: \"customPic\",\n    props: {\n        picType: {\n            type: String,\n            required: false,\n            default: \"avatar\"\n        },\n        picSrc: {\n            type: String,\n            required: false,\n            default: \"\"\n        }\n    },\n    data(){\n        return{\n            path: path,\n        }\n    },\n    computed:{\n        ...mapGetters('user', ['userInfo']),\n        avatar(){\n            if(this.picSrc === ''){\n                if(this.userInfo.headerImg !== '' && this.userInfo.headerImg.slice(0, 4) === \"http\"){\n                    return this.userInfo.headerImg\n                }\n                return this.path + this.userInfo.headerImg\n            }else{\n                if(this.picSrc !== '' && this.picSrc.slice(0, 4) === \"http\"){\n                    return this.picSrc\n                }\n                return this.path + this.picSrc\n            }\n        },\n        file(){\n            if(this.picSrc && this.picSrc.slice(0, 4) !== \"http\"){\n                return this.path + this.picSrc\n            }\n            return this.picSrc\n        }\n    }\n}\n</script>\n\n<style scoped>\n.headerAvatar{\n    display: flex;\n    justify-content: center;\n    align-items: center;\n}\n.file{\n    width: 80px;\n    height: 80px;\n    position: relative;\n}\n</style>"]}]}